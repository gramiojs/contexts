{
    "$schema": "https://biomejs.dev/schemas/1.5.3/schema.json",
    "organizeImports": {
        "enabled": true
    },
    "linter": {
        "enabled": true,
        "rules": {
            "style": {
                "noNonNullAssertion": "off"
            },
            "complexity": {
                "noStaticOnlyClass": "off"
            },
            "suspicious": {
                "noUnsafeDeclarationMerging": "off"
            },
            "recommended": true
        }
    }
}
