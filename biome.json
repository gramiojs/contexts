{
    "$schema": "https://biomejs.dev/schemas/2.3.15/schema.json",
    "linter": {
        "enabled": true,
        "rules": {
            "style": {
                "noNonNullAssertion": "off",
                "useExportType": "off"
            },
            "complexity": {
                "noStaticOnlyClass": "off"
            },
            "suspicious": {
                "noUnsafeDeclarationMerging": "off"
            },
            "recommended": true
        }
    }
}
